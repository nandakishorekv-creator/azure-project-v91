<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" 
	xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" 
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="leads-test-suite.xml" />
	<munit:test name="post-leadTest" doc:id="oy5anzza-48iq-je9u-jonp-34v7lheywb3" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="s40zzagt-1q62-g71p-04tv-egd01jwj92u" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="create-lead-001" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-lead" doc:id="1pbawnuf-pjau-vex0-sjvs-vgjlfj5l31r" name="post-lead"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="b6esoqr1-pl2h-2ibu-2m3p-c21oiy5hqs" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="get-leadTest" doc:id="meeqbj2b-93sa-n1ax-bqqr-z1pn361ggx" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="o1k8m7pb-x2s4-w69b-5k5n-xpmjfd824j" processor="salesforce:query">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="query-lead-001" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[
    {
        "Id": "001xx000003AAA",
        "Name": "Test Account",
        "Industry": "Technology"
    }
]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="get-lead" doc:id="gajmilf2-9bwv-tssx-aabk-hr4mm051noe" name="get-lead"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="g08vyzdy-s57t-8uc1-ck5y-s11m9wvjtij" actual="#[payload]" expected='#[payload is Object]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="put-leadTest" doc:id="ap7ofvzz-ewx1-6uso-6mpt-o5i6irc3yq" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="jpyu6q8z-f7et-ghw3-rwe8-eke4vw1sbj" processor="salesforce:update">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="update-lead-001" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[{"success": true, "id": "001xx000003AAA"}]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="put-lead" doc:id="ugh08mn3-xf4k-4wqj-jmvt-ffmsjdvk8d" name="put-lead"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="r6ddy03i-mboq-rxtp-2lbu-fqqd5ky2inr" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="delete-leadTest" doc:id="sb581yqv-jpcs-ou9u-cgqe-64kv6hn8im7" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="ljzhnw6v-lapc-jxp8-kii0-35xyd1jfghc" processor="salesforce:delete">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="delete-lead-001" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[{"success": true}]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="delete-lead" doc:id="dwo5tomr-jgsu-js9e-fi1t-qf98cjjrob" name="delete-lead"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="zlbvzlac-mxd7-u6f3-2iou-kgbdreueeor" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>

</mule>